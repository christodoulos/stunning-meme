          }
        }
      },
      "tags": []
    },
    "widgets": {
      "projectType": "library",
      "root": "libs/widgets",
      "sourceRoot": "libs/widgets/src",
      "prefix": "nocode",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
